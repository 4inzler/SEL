# Discord User Token (NOT a bot token!)
# ⚠️ WARNING: Using selfbots violates Discord's Terms of Service
# Get this from Discord Developer Tools > Network > Look for "authorization" header
DISCORD_USER_TOKEN=your_user_token_here

# OpenRouter API Key (required for LLM)
OPENROUTER_API_KEY=your_openrouter_key_here

# LLM Configuration (optional - defaults shown)
OPENROUTER_MAIN_MODEL=anthropic/claude-3.5-sonnet
OPENROUTER_UTIL_MODEL=anthropic/claude-3-haiku-20240307
OPENROUTER_VISION_MODEL=openai/gpt-4o-mini
OPENROUTER_MAIN_TEMP=0.8
OPENROUTER_UTIL_TEMP=0.3
OPENROUTER_TOP_P=0.9

# Memory Configuration (optional)
HIM_MEMORY_DIR=./sel_data/him_store
HIM_MEMORY_LEVELS=3
HIM_API_BASE_URL=http://localhost:8000
MEMORY_RECALL_LIMIT=10
RECENT_CONTEXT_LIMIT=20

# Agent Configuration (optional)
AGENTS_DIR=./agents

# Bot Behavior (optional)
APPROVAL_USER_ID=1329883906069102733
WHITELIST_CHANNEL_IDS=
SEL_TIMEZONE=America/Los_Angeles
INACTIVITY_PING_HOURS=48.0
INACTIVITY_PING_COOLDOWN_HOURS=24.0

# Terminal Control (optional - for system_agent)
TMUX_CONTROL_URL=http://localhost:9001
TMUX_CONTROL_TOKEN=

# SwarmUI Configuration (optional - for image generation)
SWARMUI_URL=http://localhost:7801
SWARMUI_API_KEY=

# ElevenLabs (for TTS and STT in voice channels)
ELEVENLABS_API_KEY=your_elevenlabs_key_here
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM
ELEVENLABS_MODEL=eleven_monolingual_v1
ELEVENLABS_STABILITY=0.5
ELEVENLABS_SIMILARITY=0.75
ELEVENLABS_STYLE=0.0

# ElevenLabs STT (Speech-to-Text for listening)
ELEVENLABS_STT_MODEL=eleven_multilingual_v2
STT_ENABLED=true

# Logging
RUST_LOG=info
